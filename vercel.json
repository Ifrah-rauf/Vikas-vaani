{
  "builds": [
    {
      "src": "ibmproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles_build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ibmproject/wsgi.py"
    }
  ],
  "rewrites":[
    {
      "source": "/(.*)","destination":"/"
    }
  ]
}
